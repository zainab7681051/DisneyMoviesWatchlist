<html
    class="w-mod-js w-mod-ix wf-roboto-n1-inactive wf-roboto-n3-inactive wf-roboto-n4-inactive wf-roboto-i4-inactive wf-roboto-n5-inactive wf-roboto-n7-inactive wf-roboto-n9-inactive wf-catamaran-n1-inactive wf-catamaran-n2-inactive wf-catamaran-n3-inactive wf-catamaran-n4-inactive wf-catamaran-n5-inactive wf-catamaran-n6-inactive wf-catamaran-n7-inactive wf-catamaran-n8-inactive wf-catamaran-n9-inactive wf-inactive">

<head>
    <meta charset="utf-8">
    <title>@ViewData["Title"] - DisneyMoviesWatchlist</title>
    <meta property="og:type" content="website">
    <meta content="width=device-width, initial-scale=1" name="viewport">
    <link href="https://assets.website-files.com/59f5ab35fccfea0001f70443/css/template-movies.86fd12656.css"
        rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com" rel="preconnect">
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin="anonymous">
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css?family=Roboto:100,300,regular,italic,500,700,900%7CCatamaran:100,200,300,regular,500,600,700,800,900"
        media="all">
    <script type="text/javascript">
        WebFont.load({ google: { families: ["Roboto:100,300,regular,italic,500,700,900", "Catamaran:100,200,300,regular,500,600,700,800,900"] } });
    </script>
    <script type="text/javascript">
        ! function (o, c) {
            var n = c.documentElement,
                t = " w-mod-";
            n.className += t + "js", ("ontouchstart" in o || o.DocumentTouch && c instanceof DocumentTouch) && (n.className += t + "touch")
        }(window, document);
    </script>
    <link href="https://assets.website-files.com/59f5ab35fccfea0001f70443/5a0995c47dd52a00016956d3_Favicon.png"
        rel="shortcut icon" type="image/x-icon">
    <link href="https://assets.website-files.com/59f5ab35fccfea0001f70443/5a0995c65f389a000168fbab_Webclip.jpg"
        rel="apple-touch-icon">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@24,400,1,0" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/DisneyMoviesWatchlist.styles.css" asp-append-version="true" />
</head>

<body>
    @RenderBody()
    <!-- FOOTER SECTION -->
    <div class="section footer">
        <div class="container w-container">
            <div class="footer-wrapper">
                <div class="footer-row w-row">
                    <div class="footer-column w-col w-col-4">
                        <div class="footer-title">Information</div><a href="/template-info/licensing"
                            class="footer-link">Image Licenses</a><a href="/contact" class="footer-link">Contact
                            us</a><a href="/about-us" class="footer-link">Our Authors</a>
                    </div>
                    <div class="footer-column w-col w-col-4">
                        <div class="footer-title">Discover</div><a href="/all-reviews" class="footer-link">All
                            Reviews</a><a href="/author-picks" class="footer-link">Author Picks</a><a
                            href="/new-reviews" class="footer-link">New Reviews</a>
                    </div>
                    <div class="footer-column w-col w-col-4">
                        <div class="footer-title">Community</div><a href="http://www.facebook.com" target="_blank"
                            class="footer-social-link w-inline-block">
                            <div class="footer-social-title icon"></div>
                            <div class="footer-social-title">Facebook</div>
                        </a><a href="http://www.twitter.com" target="_blank" class="footer-social-link w-inline-block">
                            <div class="footer-social-title icon"></div>
                            <div class="footer-social-title">Twitter</div>
                        </a><a href="http://www.instagram.com" target="_blank"
                            class="footer-social-link w-inline-block">
                            <div class="footer-social-title icon"></div>
                            <div class="footer-social-title">Instagram</div>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <div class="bottom-footer">
            <div class="container w-container">
                <div class="bottom-footer-paragraph">Copyright © Movie Reviews</div>
                <div class="bottom-footer-paragraph">Template design by&nbsp;<a href="http://studiocorvus.com/"
                        target="_blank" class="bottom-footer-link">Studio Corvus</a>&nbsp;-&nbsp;Powerd by <a
                        href="https://www.webflow.com/" target="_blank" class="bottom-footer-link">Webflow</a></div>
                <div class="bottom-footer-paragraph"><a href="/template-info/licensing" class="bottom-footer-link">Image
                        Licensing Info</a></div>
            </div>
        </div>
    </div>

    <!-- JS SCRIPTS -->

    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=59f5ab35fccfea0001f70443"
        type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>
    <script src="https://assets.website-files.com/59f5ab35fccfea0001f70443/js/template-movies.68b3f8b4d.js"
        type="text/javascript"></script>
    <script>
        // Wait for the badge to be created and then remove it
        setTimeout(function () {
            document.querySelectorAll('a.w-webflow-badge').forEach(function (element) {
                element.remove();
            });
        }, 6000);
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>

</html>
